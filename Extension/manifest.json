{
    "manifest_version": 2,

    "name": "NewsCluster",
    "description": "This extension gives you instant news",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "permissions": ["activeTab"],
    "web_accessible_resources": ["style.css"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["style.css"]
        }]
}
